package question_4;
import java.util.ArrayList;

public class Swap_in_ArrayList 
{
	public static void main(String[] args)
  {		
		ArrayList<Integer> in = new ArrayList<Integer>();
		in.add(10);
		in.add(20);
		in.add(30);
		in.add(40);
		in.add(50);
		in.add(60);
		in.add(70);
		System.out.println(in);
		in.set(1, 60);
		in.set(5, 20);
		System.out.println(in);
	}

}

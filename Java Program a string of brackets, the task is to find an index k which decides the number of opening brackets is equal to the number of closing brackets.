package question_17;
public class String_of_Brackets 
{

	public static void main(String[] args) 
  {	

		String s = "(((((((())))))))";
		int i ;
		int open = 0;
        int close = 0;
		for ( i = 0 ; i < s.length(); i++) {
				if(s.charAt(i) == '(') {
					open++;
				}
				if(s.charAt(i)== ')') {
					close++;
				}
			}
		if (open == close) {
			System.out.println("String contains equal brackets");	
		}
		}
	}
